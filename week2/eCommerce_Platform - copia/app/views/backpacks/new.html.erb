<body class="bodycenter">

  <header>
    <h1>New Backpack</h1>
  </header>

  <%= form_for @backpack do |x| %>
  <p class="stylemarino">
    <%= x.label :model %><br>
    <%= x.text_field :model %>
  </p>

  <p class="stylemarino">
    <%= x.label :color %><br>
    <%= x.text_area :color %>
  </p>

  <p class="stylemarino">
    <%= x.label :capacity %><br>
    <%= x.text_area :capacity %>
  </p>

  <p class="stylemarino">
    <%= x.label :number_of_pockets %><br>
    <%= x.text_area :number_of_pockets %>
  </p>
  
  <p class="stylemarino">
    <%= x.label :waterproof %><br>
    <%= x.text_area :waterproof %>
  </p>

  <p class="stylemarino">
    <%= x.label :price %><br>
    <%= x.text_area :price %>
  </p>

  <p>
    <%= x.submit %>
  </p>

  <% end %>

  <p class="style1">
    <%= link_to 'Back', backpacks_path %>
  </p>

</body>